<ExpressMailCommitmentRequest USERID="{{userID}}">
  <OriginZIP>{{originZip}}</OriginZIP>
  <DestinationZIP>{{destinationZip}}</DestinationZIP>
  <Date>{{date}}</Date>{{#if dropOffTime}}
  <DropOffTime>{{dropOffTime}}</DropOffTime>{{/if}}{{#if returnDates}}
  <ReturnDates>{{returnDates}}</ReturnDates>{{/if}}{{#if pmGuarantee}}
  <PMGuarantee>{{pmGuarantee}}</PMGuarantee>{{/if}}
</ExpressMailCommitmentRequest>